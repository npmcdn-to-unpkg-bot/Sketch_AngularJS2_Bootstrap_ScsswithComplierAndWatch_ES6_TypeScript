{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run scssc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "scssc": "sh ./scripts/scssc",
    "scssc:w": "sh ./scripts/scsscw",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "*",
    "es6-promise": "*",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "*",
    "systemjs": "^0.19.24",
    "typings": "^0.7.9",
    "zone.js": "^0.6.4"
  },
  "devDependencies": {
    "concurrently": "*",
    "lite-server": "*",
    "node-sass": "*",
    "nodemon": "*",
    "typescript": "*",
    "typings": "*"
  }
}
